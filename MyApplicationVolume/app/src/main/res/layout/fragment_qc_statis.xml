    <!-- 标签2 -->
    <TextView
        android:id="@+id/multifurcation_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Multifurcation:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/total_number_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

<!--     值2 -->
    <TextView
        android:id="@+id/multifurcation_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值2"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/multifurcation_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/loop_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Loop:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/multifurcation_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/loop_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值3"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/loop_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/missing_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Missing:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/loop_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/missing_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值4"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/missing_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/crossing_direction_error_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crossing direction error:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/missing_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/crossing_direction_error_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值5"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/crossing_direction_error_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/color_mutation_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Color mutation:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/crossing_direction_error_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/color_mutation_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值6"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/color_mutation_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/isolated_branch_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Isolated branch:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/color_mutation_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/isolated_branch_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值7"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/isolated_branch_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/angle_error_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Angle error:"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/isolated_branch_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/angle_error_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值8"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/angle_error_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/overlap_branch_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Overlapping branches(removed):"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/angle_error_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/overlap_branch_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值9"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/overlap_branch_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

    <TextView
        android:id="@+id/error_seg_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Error seg(tuned):"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@id/overlap_branch_label"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"/>

    <TextView
        android:id="@+id/error_seg_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="值10"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintTop_toTopOf="@id/error_seg_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
